{
  "kind": "collectionType",
  "collectionName": "static_pages",
  "info": {
    "singularName": "static-page",
    "pluralName": "static-pages",
    "displayName": "Static page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "regex": "^[A-Za-z0-9-]+$"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
